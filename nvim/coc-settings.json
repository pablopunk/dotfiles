{
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength": 3,
  "suggest.triggerAfterInsertEnter":true,
  "suggest.triggerCompletionWait": 300,
  "suggest.timeout": 2000,
  "suggest.maxCompleteItemCount": 10,
  "suggest.removeDuplicateItems": true,
  "suggest.disableMenu": true, // don't show source of completion
  /* "suggest.disableKind": true, // don't show type of item (f, v...) */
  "signature.enable": true,
  "signature.target": "float",
  "diagnostic.enable": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.virtualText": false,
  "diagnostic.level": "warning",
  "diagnostic.enableSign": false,
  "diagnostic.messageTarget": "float",
  "diagnostic.checkCurrentLine": true,
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "tslint.autoFixOnSave": true,
  "eslint.autoFixOnSave": true,
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 120,
  "prettier.requireConfig": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "javascriptreact", "html", "json", "jsonc", "scss", "css"],
  "hover.target": "float",
  "coc.preferences.highlightTimeout": 400,
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "javascript.updateImportsOnFileMove.enable": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addGBlameToBufferVar": true,
  "codeLens.enable": true,
  "typescript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,
  "snippets.priority": 1
}
