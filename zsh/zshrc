#
# OH-MY-ZSH
#
export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME="intheloop"
plugins=(git osx npm)
source $ZSH/oh-my-zsh.sh

#
# USER CONFIG
#
source ~/.functions
source ~/.aliases
source ~/.path
F=$HOME/.additional; [ -f $F ] && . $F # aditional local config

export CPPFLAGS="-I/usr/local/opt/openssl/include"

# GPG
export GPG_TTY=$(tty)
