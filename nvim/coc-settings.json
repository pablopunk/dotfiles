{
  "suggest.autoTrigger": "trigger",
  "suggest.maxCompleteItemCount": 10,
  "suggest.removeDuplicateItems": true,
  "signature.enable": true,
  "signature.target": "float",
  "diagnostic.enable": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.virtualText": true,
  "diagnostic.level": "warning",
  "diagnostic.enableSign": false,
  "diagnostic.messageTarget": "float",
  "diagnostic.checkCurrentLine": true,
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "tslint.autoFixOnSave": true,
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.eslintIntegration": false,
  "prettier.requireConfig": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "javascriptreact", "html"],
  "coc.preferences.hoverTarget": "float",
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "javascript.updateImportsOnFileMove.enable": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addGBlameToBufferVar": true
}
