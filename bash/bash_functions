function powersave {
  # linux
  sudo cpufreq-set -g powersave && \
    sudo pm-powersave true
}

function power {
  sudo cpufreq-set -g performance && \
    sudo pm-powersave false
}

function gclone {
  if [ "$1" = "pablopunk" ]; then
    git clone git@github.com:pablopunk/$2
  else
    git clone https://github.com/$1/$2
  fi
}

function grebase {
  git fetch upstream && \
    git rebase upstream/master
}
