{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",

  "agents": {
    // ============================================================
    // MAIN ORCHESTRATION
    // ============================================================

    // üéØ THE BOSS: Main orchestrator - coordinates all work, delegates to specialized agents
    "sisyphus": {
      "model": "anthropic/claude-opus-4-6"
    },

    // üìã THE PLANNER: Creates detailed work plans by interviewing you (Press Tab to use)
    "prometheus": {
      "model": "openai/gpt-5.2"
    },

    // ============================================================
    // SPECIALIZED WORKERS
    // ============================================================

    // üî® THE WORKER: Deep autonomous worker for heavy lifting & end-to-end implementation
    "hephaestus": {
      "model": "openai/gpt-5.3-codex"
    },

    // üêõ THE DEBUGGER: Architecture & debugging expert for complex issues
    "oracle": {
      "model": "anthropic/claude-opus-4-6"
    },

    // üìö THE RESEARCHER: Explores documentation & understands open source code patterns
    "librarian": {
      "model": "anthropic/claude-sonnet-4-5"
    },

    // üîç THE SCOUT: Fast pattern search & codebase exploration (speed optimized)
    "explore": {
      "model": "anthropic/claude-haiku-4-5"
    },

    // üé® THE DESIGNER: UI/visual design tasks with visual reasoning
    "multimodal-looker": {
      "model": "google/gemini-2.5-flash"
    },

    // ============================================================
    // VALIDATION & QUALITY
    // ============================================================

    // ‚úÖ THE REVIEWER: Validates plans for feasibility & best practices
    "metis": {
      "model": "anthropic/claude-sonnet-4-5"
    },

    // üìù THE CRITIC: Code quality checker - prevents excessive comments & boilerplate
    "momus": {
      "model": "anthropic/claude-haiku-4-5"
    },

    // ‚öôÔ∏è THE EXECUTOR: Task executor & result verifier for systematic completion
    "atlas": {
      "model": "anthropic/claude-sonnet-4-5"
    }
  },

  "categories": {
    // ============================================================
    // TASK CATEGORY ROUTING
    // ============================================================

    // üé® Visual/frontend tasks
    "visual-engineering": {
      "model": "google/gemini-2.5-flash"
    },

    // üß† Deep thinking & complex logic
    "ultrabrain": {
      "model": "anthropic/claude-opus-4-6"
    },

    // üî¨ Complex research & exploration
    "deep": {
      "model": "anthropic/claude-opus-4-6"
    },

    // ‚ú® Creative/design tasks
    "artistry": {
      "model": "google/gemini-2.5-flash"
    },

    // ‚ö° Fast & simple tasks (cost optimized)
    "quick": {
      "model": "anthropic/claude-haiku-4-5"
    },

    // üìä Low-complexity unspecified tasks
    "unspecified-low": {
      "model": "anthropic/claude-haiku-4-5"
    },

    // üéØ High-complexity unspecified tasks
    "unspecified-high": {
      "model": "anthropic/claude-opus-4-6"
    },

    // ‚úçÔ∏è Writing & documentation
    "writing": {
      "model": "anthropic/claude-sonnet-4-5"
    }
  },

  // TMUX: Visual multi-agent in separate panes
  // Requires: running inside tmux with `opencode --port 4096`
  "tmux": {
    "enabled": true,
    "layout": "main-vertical",
    "main_pane_size": 60
  },

  // COST CONTROL: Limit parallel background agents
  "background_task": {
    "defaultConcurrency": 5,
    "staleTimeoutMs": 180000,
    "modelConcurrency": {
      "anthropic/claude-opus-4-6": 2,
      "anthropic/claude-sonnet-4-5": 4,
      "anthropic/claude-haiku-4-5": 10,
      "openai/gpt-5.2": 3,
      "google/gemini-2.5-flash": 10
    }
  }
}
