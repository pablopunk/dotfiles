#
# OH-MY-ZSH
#
export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME=""
plugins=(git osx npm)
source $ZSH/oh-my-zsh.sh

#
# PROMPT
#
eval "$(starship init zsh)"

#
# USER CONFIG
#
source ~/.functions
source ~/.aliases
source ~/.path
# $HOME/.hello| lolcat
F=$HOME/.additional; [ -f $F ] && . $F # aditional local config

# Vars
export EDITOR=nvim

[[ "$(uname)" == "Darwin" ]] && eval "$(pyenv init -)" # python versions
# linuxbrew
export PATH="/home/linuxbrew/.linuxbrew/opt/node@10/bin:$PATH"
