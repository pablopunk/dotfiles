{
  "window.commandCenter": 1,
  "editor.fontFamily": "Source Code Pro for Powerline, SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "window.customTitleBarVisibility": "never",
  "window.titleBarStyle": "native",
  "window.nativeTabs": true,
  "cursor.cpp.disabledLanguages": ["scminput"],
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "git.enableSmartCommit": true,
  "diffEditor.renderSideBySide": false,
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "NONE",
  "remote.SSH.remotePlatform": {
    "nuke": "linux"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "editor.accessibilitySupport": "off",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.env.osx": {},
  "console-ninja.featureSet": "Community",
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Monochrome Dark Subtle",
  "workbench.preferredLightColorTheme": "Monochrome Light Subtle",
  "workbench.preferredDarkColorTheme": "Monochrome Dark Subtle",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  }
}
