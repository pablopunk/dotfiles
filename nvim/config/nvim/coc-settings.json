{
  "diagnostic.virtualText": false,
  "diagnostic.warningSign": "•",
  "diagnostic.errorSign": "•",
  "diagnostic.infoSign": "•",
  "diagnostic.hintSign": "•",
  "diagnostic.messageTarget": "float",
  "diagnostic.level": "error",
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "tslint.autoFixOnSave": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "typescript",
    "typescript.jsx"
  ]
}
