{
  "window.commandCenter": 1,
  "editor.fontFamily": "Jetbrains Mono, Source Code Pro for Powerline, SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "window.customTitleBarVisibility": "never",
  "window.titleBarStyle": "custom",
  "window.nativeTabs": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "git.enableSmartCommit": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  // "vscode-neovim.neovimInitVimPaths.darwin": "NONE",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "biomejs.biome",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[html]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.env.osx": {},
  "editor.formatOnSave": true,
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "Cursor Dark Midnight",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "keyboard.dispatch": "keyCode",
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "typescript.format.enable": false,
  "cursor.cmdk.useThemedDiffBackground": true,
  "cursor.diffs.useCharacterLevelDiffs": true,
  "workbench.preferredLightColorTheme": "GitHub Light",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.renderSideBySide": false,
  "cursor.cpp.enablePartialAccepts": true,
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cursor.cpp.disabledLanguages": [
    "plaintext"
  ]
}
