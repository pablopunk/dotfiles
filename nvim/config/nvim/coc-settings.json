{
  "suggest.autoTrigger": "always",
  "suggest.triggerAfterInsertEnter": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.enablePreview": true,
  "suggest.disableMenu": true,
  "suggest.removeDuplicateItems": true,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.virtualText": false,
  "diagnostic.level": "error",
  "diagnostic.warningSign": "·",
  "diagnostic.errorSign": "⨯",
  "diagnostic.infoSign": "¡",
  "diagnostic.hintSign": "•",
  "diagnostic.messageTarget": "float",
  "diagnostic.checkCurrentLine": true,
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "tslint.autoFixOnSave": false,
  "eslint.autoFixOnSave": true,
  "eslint.enable": false,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.eslintIntegration": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "javascriptreact", "html"],
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "javascript.updateImportsOnFileMove.enable": true,
  "pairs.enableBackspace": true,
  "emmet.includeLanguages": { "javascript": "javascriptreact", "typescript": "typescriptreact" },
  "emmet.optimizeStylesheetParsing": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addGBlameToBufferVar": true,
  "python.linting.flake8Enabled": true
}
