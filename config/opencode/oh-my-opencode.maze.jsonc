{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    // ğŸ¯ THE BOSS: Main orchestrator - coordinates all work, delegates to specialized agents
    "sisyphus": {
      "model": "anthropic/claude-sonnet-4-5"
    },

    // ğŸ“‹ THE PLANNER: Creates detailed work plans by interviewing you (Press Tab to use)
    "prometheus": {
      "model": "openai/gpt-5.3-codex"
    },

    // ğŸ”¨ THE WORKER: Deep autonomous worker for heavy lifting & end-to-end implementation
    "hephaestus": {
      "model": "openai/gpt-5.3-codex"
    },

    // ğŸ› THE DEBUGGER: Architecture & debugging expert for complex issues
    "oracle": {
      "model": "anthropic/claude-opus-4-6"
    },

    // ğŸ“š THE RESEARCHER: Explores documentation & understands open source code patterns
    "librarian": {
      "model": "anthropic/claude-sonnet-4-5"
    },

    // ğŸ” THE SCOUT: Fast pattern search & codebase exploration (speed optimized)
    "explore": {
      "model": "anthropic/claude-haiku-4-5"
    },

    // ğŸ¨ THE DESIGNER: UI/visual design tasks with visual reasoning
    "multimodal-looker": {
      "model": "anthropic/claude-opus-4-6"
    },

    // ============================================================
    // VALIDATION & QUALITY
    // ============================================================

    // âœ… THE REVIEWER: Validates plans for feasibility & best practices
    "metis": {
      "model": "anthropic/claude-sonnet-4-5"
    },

    // ğŸ“ THE CRITIC: Code quality checker - prevents excessive comments & boilerplate
    "momus": {
      "model": "anthropic/claude-sonnet-4-5"
    },

    // âš™ï¸ THE EXECUTOR: Task executor & result verifier for systematic completion
    "atlas": {
      "model": "anthropic/claude-haiku-4-5"
    }
  },

  // TMUX: Visual multi-agent in separate panes
  // Requires: running inside tmux with `opencode --port 4096`
  "tmux": {
    "enabled": true,
    "layout": "main-vertical",
    "main_pane_size": 60
  },

  // COST CONTROL: Limit parallel background agents
  // "background_task": {
  //   "defaultConcurrency": 5,
  //   "staleTimeoutMs": 180000,
  //   "modelConcurrency": {
  //     "anthropic/claude-opus-4-6": 2,
